{
  "name": "shizu_backend",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node lib/index.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json --fix",
    "start-watch": "nodemon lib/index.js --inspect=0.0.0.0:9229",
    "start-wait-debuger": "nodemon lib/index.js --inspect-brk=0.0.0.0:9229",
    "build-api": "openapi-typescript ./openapi/schema.yaml -o ./openapi/schema.d.ts",
    "build-redoc": "redocly build-docs openapi/schema.yaml -o openapi/redoc-static.html" 
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@redocly/cli": "^1.22.1",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-graphql": "^0.12.0",
    "express-openapi": "^12.1.3",
    "fluent-ffmpeg": "^2.1.3",
    "graphql-http": "^1.22.1",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.4",
    "openapi-typescript": "^7.4.0",
    "pg": "^8.12.0",
    "pg-format": "^1.0.4",
    "ruru": "^2.0.0-beta.14"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.11",
    "@types/pg": "^8.11.6",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "tslint": "^6.1.3",
    "typescript": "^5.5.3"
  }
}
