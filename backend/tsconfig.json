{
    "compilerOptions": {
      "target": "es2020",
      "module": "node16",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./lib",
      "moduleResolution": "node16"
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
  }